<%= form_for [@user, @photo] do |f| %>
    <%= f.label :description %>
    <%= f.text_area :description %>
    <%= f.label :categories %>
    <%= f.collection_check_boxes(:category_ids, Category.all, :id, :name) do |b| %>
        <p>
        <%= b.check_box %>
        <%= b.label %>
        </p>
        <%end%>
    <%= f.label :image %>
    <%= f.file_field :image%>
    <%= f.submit "Upload Image" %>
    <%end%>

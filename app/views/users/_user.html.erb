<%= form_for (user) do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %>
  <%= f.label :username %><br>
  <%= f.text_field :username %>
  <%= f.label :email %><br>
  <%= f.text_field :email %>
  <%= f.label :password %><br>
  <%= f.password_field :password%>
  <%= f.label :avatar %><br>
  <%= f.file_field :avatar %>
  <br><br>
  <%= f.submit %>



<% end %>


